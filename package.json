{
  "name": "n8n-nodes-mautic",
  "version": "0.1.0",
  "description": "N8N node for Mautic",
  "keywords": [
    "n8n-community-node-package"
  ],
  "license": "MIT",
  "homepage": "https://github.com/Agilicus/n8n-nodes-mautic.git",
  "author": {
    "name": "Agilicus Devs",
    "email": "dev@agilicus.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Agilicus/n8n-nodes-mautic.git"
  },
  "engines": {
    "node": ">=20.15"
  },
  "main": "index.js",
  "scripts": {
    "build": "npx rimraf dist && tsc && gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier nodes credentials --write",
    "lint": "eslint nodes credentials package.json",
    "lintfix": "eslint nodes credentials package.json --fix",
    "prepublishOnly": "npm run build && npm run lint -c .eslintrc.prepublish.js nodes credentials package.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/n8n-nodes-mautic/MauticRestApi.credentials.js",
      "dist/credentials/n8n-nodes-mautic/MauticOAuth2RestApi.credentials.ts"
    ],
    "nodes": [
      "dist/nodes/n8n-nodes-mautic/Mautic.node.js"
    ]
  },
  "devDependencies": {
    "@types/change-case": "^0.0.30",
    "@types/jest": "^29.5.12",
    "@types/node": "^24.3.1",
    "@typescript-eslint/parser": "~8.32.0",
    "eslint": "^8.57.0",
    "eslint-plugin-n8n-nodes-base": "^1.16.3",
    "gulp": "^5.0.0",
    "jest": "^29.7.0",
    "jest-environment-node": "^29.7.0",
    "n8n-workflow": "^1.69.0",
    "prettier": "^3.5.3",
    "ts-jest": "^29.1.4",
    "typescript": "^5.8.2"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  },
  "dependencies": {
    "change-case": "^5.4.4",
    "snake-case": "^3.0.4"
  }
}
